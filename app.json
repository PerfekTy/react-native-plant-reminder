{
  "expo": {
    "name": "Plant Reminder",
    "slug": "Plant Reminder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "notification": {
      "icon": "./app/assets/icon.png"
    },
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.perfekt.rn"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1cddb3eb-0aa6-4e75-a2d8-dcb60dbeb026"
      }
    },
    "plugins": ["expo-font"]
  }
}
